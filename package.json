{
  "name": "mytinerary",
  "version": "1.0.0",
  "description": "mytinerary",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config index.js",
    "client": "cd client && npm start",
    "server": "nodemon -r dotenv/config index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
