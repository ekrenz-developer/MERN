  "bugs": {
    "url": "https://github.com/ekrenz-developer/MYtinerary/issues"
  },
  "homepage": "https://github.com/ekrenz-developer/MYtinerary#readme",
  "dependencies": {
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.7"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}
